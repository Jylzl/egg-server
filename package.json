{
  "name": "server",
  "version": "1.0.0",
  "description": "a blog server",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "await-stream-ready": "^1.0.1",
    "egg": "^2.27.0",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-passport": "^2.1.1",
    "egg-passport-github": "^1.0.0",
    "egg-scripts": "^2.13.0",
    "egg-sequelize": "^5.2.2",
    "egg-validate": "^2.0.2",
    "image-downloader": "^4.0.1",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.15.0",
    "egg-ci": "^1.15.0",
    "egg-mock": "^4.0.0",
    "eslint": "^7.5.0",
    "eslint-config-egg": "^8.0.1",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-server",
    "stop": "egg-scripts stop --title=egg-server-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "egg-sequelize migration:create --name init",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jylzl/egg-server"
  },
  "author": "lizilong<94648929@qq.com>",
  "license": "MIT"
}
